{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:List*",
        "s3:GetBucket*"
      ],
      "Resource": [
        "arn:aws:s3:::olm-pics-s3",
        "arn:aws:s3:::olm-pics-s3/*",
        "arn:aws:s3:::olm-processed-s3",
        "arn:aws:s3:::olm-processed-s3/*",
        "arn:aws:s3:::open-logo-s3",
        "arn:aws:s3:::open-logo-s3/*",
        "arn:aws:s3:::logodetect-model-s3",
        "arn:aws:s3:::logodetect-model-s3/*"
      ]
    }
  ]
}
