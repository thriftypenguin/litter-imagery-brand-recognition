   "source": [
    "SELECT \n",
    "  brand_id AS brand_id,\n",
    "  ROUND (SUM(qty)*100.0/(SELECT SUM(qty) FROM brand_image), 5) AS qty_fraction\n",
    "FROM \n",
    "  brand_image\n",
    "GROUP BY brand_id\n",
    "ORDER BY qty_fraction DESC"
   ]
  }
 ],
